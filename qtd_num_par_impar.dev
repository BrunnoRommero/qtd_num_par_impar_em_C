using namespace std;

int main (void){
  int i;
	int cont_impar=0;
	int cont_par=0;
	
	  for(i=1;i<=130;i++){
	    if(i%2==0){
	      cont_par=cont_par+1;
	      cout <<i<<endl;
	      cout <<" Par "<<endl;
	    }
	    else{
	      cont_impar=cont_impar+1;
	      cout<<i<<endl;
	      cout<<" Impar"<<endl;
	      
	    }
	  }

    cout <<"Quantidade de Numeros par  "<<cont_par<<endl;
    cout <<"Quantidade de Numeros impar  "<<cont_impar<<endl;
}
